@startuml
abstract class Entity {
    - textureName : string
    # sprite : Sprite
    # Velocity : Vector2f
    + Entity(textureName:string)
    + <<virtual>> Create() : void
    + <<virtual>> Move(scene:Scene, deltaTime:float) : void
    + <<abstract>> Update(deltaTime:float) : void
    + Render(window:RenderWindow) : void
}
AssetManager::LoadTexture <- Entity::Create
@enduml
